
{
    "manifest_version": 3,
    "name": "AutoFill Extension",
    "version": "1.0",
    "description": "Auto-fill forms with local data",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "AutoFill"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "host_permissions": ["<all_urls>"]
}
